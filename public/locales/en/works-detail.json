          {
            "tech": "Apache Airflow",
            "description": "Schedule and orchestrate DAG."
          },
          {
            "tech": "Postgres",
            "description": "Store Airflow details and user purchase data."
          },
          {
            "tech": "DuckDB",
            "description": "Data warehouse for processing."
          },
